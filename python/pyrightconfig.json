{
  "include": [
    "src",
    "tests"
  ],
  "exclude": [
    "**/__pycache__",
    "**/.pytest_cache",
    "build",
    "dist",
    ".venv"
  ],
  "extraPaths": [
    "."
  ],
  "typeCheckingMode": "basic",
  "pythonVersion": "3.12",
  "pythonPlatform": "All",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "useLibraryCodeForTypes": true,
  "autoSearchPaths": true,
  "venvPath": ".",
  "venv": ".venv",
  "stubPath": "typings"
}